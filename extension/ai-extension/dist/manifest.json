{
  "manifest_version": 3,
  "name": "SENTRY AI",
  "version": "1.0",
  "description": "Sentry AI - Your Intelligent Web Protector",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
        "16": "images/NOBG/Sentry_Logo-removebg-preview.png",
        "48": "images/NOBG/Sentry_Logo-removebg-preview.png",
        "128": "images/NOBG/Sentry_Logo-removebg-preview.png"
    }
  },
  "icons": {
    "16": "images/NOBG/Sentry_Logo-removebg-preview.png",
    "48": "images/NOBG/Sentry_Logo-removebg-preview.png",
    "128": "images/NOBG/Sentry_Logo-removebg-preview.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "identity"],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
  "oauth2": {
    "client_id": "768177512844-YOUR_OAUTH_CLIENT_ID.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "http://localhost/*",
        "http://127.0.0.1/*",
        "https://localhost/*",
        "https://127.0.0.1/*"
      ],
      "js": [
        "contentscript_new.js",
        "chatbotUI.js"
        ],
      "css": [
        "css/chatbotUI.css", 
        "css/notifications.css",
        "css/content-blocker.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/WithBG/Happy_Sentry.png",
        "images/NOBG/Sentry_Logo-removebg-preview.png",
        "images/NOBG/Sentry_Chat.png",
        "css/notifications.css"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}